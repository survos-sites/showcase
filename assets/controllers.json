{
    "controllers": {
        "@survos/bootstrap-bundle": {
            "search": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            }
        },
        "@survos/meili-bundle": {
            "insta": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "instantsearch.css/themes/algolia.min.css": true,
                    "pretty-print-json/dist/css/pretty-print-json.min.css": true
                }
            },
            "json": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": []
            }
        },
        "@survos/reveal-bundle": {
            "reveal": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "reveal.js/dist/reveal.js": true,
                    "reveal.js/dist/reveal.css": true,
                    "reveal.js/dist/theme/black.css": true
                }
            }
        },
        "@survos/simple-datatables-bundle": {
            "table": {
                "enabled": true,
                "fetch": "lazy",
                "autoimport": {
                    "@survos/simple-datatables/style.css": true
                }
            }
        },
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        }
    },
    "entrypoints": []
}
